  <DropShadowEffect x:Key="BackgroundBlur.Effect.TextShadow" Direction="270" BlurRadius="2" ShadowDepth="1" Opacity="0.2"/>
  <SolidColorBrush x:Key="BackgroundBlur.Brush.Slider.Static.Border" Color="#FFCBE5FF"/>
  <SolidColorBrush x:Key="BackgroundBlur.Brush.Slider.MouseOver.Border" Color="White"/>
  <SolidColorBrush x:Key="BackgroundBlur.Brush.SliderThumb.Static.Border" Color="Black"/>
  <SolidColorBrush x:Key="BackgroundBlur.Brush.SliderThumb.MouseOver.Background" Color="#FFF0F0F0"/>
  <SolidColorBrush x:Key="BackgroundBlur.Brush.SliderThumb.MouseOver.Border" Color="#FF316AA3"/>
  <SolidColorBrush x:Key="BackgroundBlur.Brush.SliderThumb.Pressed.Border" Color="#FF0E4378"/>
  <SolidColorBrush x:Key="BackgroundBlur.Brush.SliderThumb.Static.Background" Color="#FFDCECFC"/>

  <Style x:Key="BackgroundBlur.Style.RepeatButtonTransparent" TargetType="{x:Type RepeatButton}">
    <Setter Property="OverridesDefaultStyle" Value="true"/>
    <Setter Property="Background" Value="Transparent"/>
    <Setter Property="Focusable" Value="false"/>
    <Setter Property="IsTabStop" Value="false"/>
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type RepeatButton}">
          <Rectangle Fill="{TemplateBinding Background}" Height="{TemplateBinding Height}" Width="{TemplateBinding Width}"/>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <ControlTemplate x:Key="BackgroundBlur.Template.SliderThumb" TargetType="{x:Type Thumb}">
    <Rectangle x:Name="grip" Fill="{StaticResource BackgroundBlur.Brush.SliderThumb.Static.Background}" Stroke="{StaticResource BackgroundBlur.Brush.SliderThumb.Static.Border}" StrokeThickness="0.5"
               Width="5" Height="12" Stretch="Fill" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    <ControlTemplate.Triggers>
      <Trigger Property="IsMouseOver" Value="true">
        <Setter Property="Stroke" TargetName="grip" Value="{StaticResource BackgroundBlur.Brush.SliderThumb.MouseOver.Border}"/>
      </Trigger>
      <Trigger Property="IsDragging" Value="true">
        <Setter Property="Fill" TargetName="grip" Value="{StaticResource BackgroundBlur.Brush.Foreground}"/>
        <Setter Property="Stroke" TargetName="grip" Value="{StaticResource BackgroundBlur.Brush.SliderThumb.Pressed.Border}"/>
      </Trigger>
      <Trigger Property="Tag" Value="FocusEngaged">
        <Setter Property="Fill" TargetName="grip" Value="{StaticResource BackgroundBlur.Brush.Foreground}"/>
        <Setter Property="Stroke" TargetName="grip" Value="{StaticResource BackgroundBlur.Brush.SliderThumb.Pressed.Border}"/>
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <ControlTemplate x:Key="BackgroundBlur.Template.SliderHorizontal" TargetType="{x:Type Slider}">
    <Border BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}" Background="{TemplateBinding Background}" SnapsToDevicePixels="True">
      <Grid>
        <Border x:Name="border" SnapsToDevicePixels="False" BorderBrush="{StaticResource BackgroundBlur.Brush.Slider.Static.Border}" BorderThickness="0.2" Height="6" Margin="0,2" VerticalAlignment="Center"/>
        <Track x:Name="PART_Track">
          <Track.DecreaseRepeatButton>
            <RepeatButton Command="{x:Static Slider.DecreaseLarge}" Style="{StaticResource BackgroundBlur.Style.RepeatButtonTransparent}" Margin="1,4,0,4" Background="{StaticResource BackgroundBlur.Brush.Foreground}"/>
          </Track.DecreaseRepeatButton>
          <Track.IncreaseRepeatButton>
            <RepeatButton Command="{x:Static Slider.IncreaseLarge}" Style="{StaticResource BackgroundBlur.Style.RepeatButtonTransparent}"/>
          </Track.IncreaseRepeatButton>
          <Track.Thumb>
            <Thumb x:Name="Thumb" Focusable="False" Template="{StaticResource BackgroundBlur.Template.SliderThumb}" VerticalAlignment="Center"/>
          </Track.Thumb>
        </Track>
      </Grid>
    </Border>
    <ControlTemplate.Triggers>
      <Trigger Property="IsKeyboardFocused" Value="true">
        <Setter Property="BorderBrush" TargetName="border" Value="{StaticResource BackgroundBlur.Brush.Slider.MouseOver.Border}"/>
        <Setter Property="BorderThickness" TargetName="border" Value="0.4"/>
      </Trigger>
      <Trigger Property="noesis:Element.IsFocusEngaged" Value="true">
        <Setter Property="Tag" TargetName="Thumb" Value="FocusEngaged"/>
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <Style x:Key="BackgroundBlur.Slider" TargetType="{x:Type Slider}">
    <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
    <Setter Property="Background" Value="Transparent"/>
    <Setter Property="BorderBrush" Value="Transparent"/>
    <Setter Property="Minimum" Value="0"/>
    <Setter Property="Maximum" Value="40"/>
    <Setter Property="SmallChange" Value="1"/>
    <Setter Property="LargeChange" Value="10"/>
    <Setter Property="IsMoveToPointEnabled" Value="True"/>
    <Setter Property="Template" Value="{StaticResource BackgroundBlur.Template.SliderHorizontal}"/>
    <Setter Property="noesis:StyleInteraction.Triggers">
      <Setter.Value>
        <noesis:StyleTriggerCollection>
          <b:EventTrigger EventName="MouseEnter">
            <noesis:SetFocusAction/>
          </b:EventTrigger>
        </noesis:StyleTriggerCollection>
      </Setter.Value>
    </Setter>
  </Style>

  <Style x:Key="BackgroundBlur.ButtonBlue" TargetType="{x:Type Button}">
    <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
    <Setter Property="HorizontalContentAlignment" Value="Center"/>
    <Setter Property="VerticalContentAlignment" Value="Center"/>
    <Setter Property="FontSize" Value="11"/>
    <Setter Property="BorderBrush" Value="{StaticResource BackgroundBlur.Brush.ButtonBlue.Border}"/>
    <Setter Property="Foreground" Value="#E9F6F9"/>
    <Setter Property="Background">
      <Setter.Value>
        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
          <GradientStop Color="#194887" Offset="0"/>
          <GradientStop Color="#1D6FB7" Offset="1"/>
        </LinearGradientBrush>
      </Setter.Value>
    </Setter>
    <Setter Property="noesis:StyleInteraction.Triggers">
      <Setter.Value>
        <noesis:StyleTriggerCollection>
          <b:EventTrigger EventName="MouseEnter">
            <noesis:SetFocusAction/>
          </b:EventTrigger>
        </noesis:StyleTriggerCollection>
      </Setter.Value>
    </Setter>
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type Button}">
          <Viewbox x:Name="viewbox" Stretch="Uniform" RenderTransformOrigin="0.5,0.5">
            <Viewbox.RenderTransform>
              <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform/>
                <TranslateTransform/>
              </TransformGroup>
            </Viewbox.RenderTransform>
            <VisualStateManager.VisualStateGroups>
              <VisualStateGroup x:Name="FocusStates">
                <VisualStateGroup.Transitions>
                  <VisualTransition GeneratedDuration="0:0:0.1" To="Focused"/>
                  <VisualTransition From="Focused" GeneratedDuration="0:0:0.1"/>
                </VisualStateGroup.Transitions>
                <VisualState x:Name="Unfocused"/>
                <VisualState x:Name="Focused">
