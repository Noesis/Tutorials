$(BINDIR)/Samples.BlendTutorial: $(PREFIX)/Samples/BlendTutorial/Projects/linux_x86/_build/Profile/App.xaml.bin.h $(PREFIX)/Samples/BlendTutorial/Projects/linux_x86/_build/Profile/MainWindow.xaml.bin.h $(PREFIX)/Samples/BlendTutorial/Projects/linux_x86/_build/Profile/ColorSelector.xaml.bin.h $(PREFIX)/Samples/BlendTutorial/Projects/linux_x86/_build/Profile/Resources.xaml.bin.h $(PREFIX)/Samples/BlendTutorial/Projects/linux_x86/_build/Profile/WeblySleek_UI_Normal.ttf.bin.h $(PREFIX)/Samples/BlendTutorial/Projects/linux_x86/_build/Profile/Main.o $(BINDIR)/libNoesis.so $(BINDIR)/libNoesisApp.so
	@echo [LD] $(notdir $@)
	$(Q)g++ $(LFLAGS_PROFILE) -m32 -pie -Wl,-rpath,\$$ORIGIN -Wl,-rpath,$(PREFIX)/../../Bin/linux_x86 -L$(PREFIX)/../../Bin/linux_x86  -o $(BINDIR)/Samples.BlendTutorial $(PREFIX)/Samples/BlendTutorial/Projects/linux_x86/_build/Profile/Main.o -lNoesisApp -lNoesis
	@echo [STRIP] $(notdir $@)
	$(Q)objcopy --only-keep-debug $(BINDIR)/Samples.BlendTutorial $(BINDIR)/Samples.BlendTutorial.debug 
	$(Q)strip --strip-unneeded $(BINDIR)/Samples.BlendTutorial
	$(Q)objcopy --add-gnu-debuglink=$(BINDIR)/Samples.BlendTutorial.debug $(BINDIR)/Samples.BlendTutorial

PROFILE_CLEAN_SAMPLES_BLENDTUTORIAL:
	@rm -f $(PREFIX)/Samples/BlendTutorial/Projects/linux_x86/_build/Profile/Main.o $(PREFIX)/Samples/BlendTutorial/Projects/linux_x86/_build/Profile/App.xaml.bin.h $(PREFIX)/Samples/BlendTutorial/Projects/linux_x86/_build/Profile/MainWindow.xaml.bin.h $(PREFIX)/Samples/BlendTutorial/Projects/linux_x86/_build/Profile/ColorSelector.xaml.bin.h $(PREFIX)/Samples/BlendTutorial/Projects/linux_x86/_build/Profile/Resources.xaml.bin.h $(PREFIX)/Samples/BlendTutorial/Projects/linux_x86/_build/Profile/WeblySleek_UI_Normal.ttf.bin.h $(PREFIX)/Samples/BlendTutorial/Projects/linux_x86/_build/Profile/pch.h.gch
	@rm -f $(PREFIX)/Samples/BlendTutorial/Projects/linux_x86/_build/Profile/pch.h.d $(PREFIX)/Samples/BlendTutorial/Projects/linux_x86/_build/Profile/Main.d
	@rm -f $(BINDIR)/Samples.BlendTutorial
	@rm -f $(BINDIR)/Samples.BlendTutorial.debug

