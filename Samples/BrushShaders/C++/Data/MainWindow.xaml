      <GradientStop Offset="0.3" Color="#FF0C3251"/>
      <GradientStop Offset="0.7" Color="#FF0C3251"/>
      <GradientStop Offset="1" Color="#FF124C7A"/>
    </LinearGradientBrush>
  </Window.Background>

