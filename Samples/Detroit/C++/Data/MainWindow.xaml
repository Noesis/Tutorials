          <StackPanel Height="{Binding ActualHeight, ElementName=OptionsTitleGroup}">
            <StackPanel Orientation="Horizontal">
              <Rectangle Width="3" Height="3" Fill="#E53D434A"/>
              <Rectangle Width="158" Height="1" Fill="#343D434A" VerticalAlignment="Bottom"/>
            </StackPanel>
          </StackPanel>
        </Viewbox>
      </Grid>
    </Grid>
    <Grid x:Name="MessageDialog" Grid.Row="0" Grid.RowSpan="4" Visibility="Collapsed">
      <Rectangle x:Name="MessageDialogBg" Fill="#80FFFFFF"/>
      <Grid>
        <Grid.RowDefinitions>
          <RowDefinition Height="4*"/>
          <RowDefinition Height="3*"/>
          <RowDefinition Height="3*"/>
          <RowDefinition Height="4*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
          <ColumnDefinition Width="*"/>
          <ColumnDefinition Width="Auto"/>
          <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Decorator x:Name="MessageDialogHalfRef" Grid.Row="1"/>
        <StackPanel x:Name="MessageDialogRef" Grid.Row="1" Grid.RowSpan="2" Grid.Column="1" Orientation="Horizontal">
          <Decorator Grid.Column="1" Width="{Binding ActualHeight, ElementName=MessageDialogHalfRef}"/>
          <Decorator Grid.Column="2" Width="{Binding ActualHeight, ElementName=MessageDialogHalfRef}"/>
          <Decorator Grid.Column="3" Width="{Binding ActualHeight, ElementName=MessageDialogHalfRef}"/>
          <Decorator Grid.Column="4" Width="{Binding ActualHeight, ElementName=MessageDialogHalfRef}"/>
          <Decorator Grid.Column="5" Width="{Binding ActualHeight, ElementName=MessageDialogHalfRef}"/>
        </StackPanel>
      </Grid>
      <Grid x:Name="MessageContent" Width="{Binding ActualWidth, ElementName=MessageDialogRef}" Height="{Binding ActualHeight, ElementName=MessageDialogRef}">
        <Grid>
          <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
          </Grid.ColumnDefinitions>
          <Path Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="2" Data="M0,0L2,0 1,1z" Fill="#A0FFFFFF" Stretch="Fill"/>
          <Path Grid.Row="1" Grid.Column="1" Grid.ColumnSpan="2" Data="M1,0L2,1 0,1z" Fill="#E0FFFFFF" Stretch="Fill"/>
          <Path Grid.Row="1" Grid.Column="2" Grid.ColumnSpan="2" Data="M0,0L2,0 1,1z" Fill="#C0FFFFFF" Stretch="Fill"/>
          <Path Grid.Row="1" Grid.Column="3" Grid.ColumnSpan="2" Data="M1,0L2,1 0,1z" Fill="#80FFFFFF" Stretch="Fill"/>
        </Grid>
        <Grid>
          <Grid.RowDefinitions>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="5*"/>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="6*"/>
            <RowDefinition Height="3*"/>
          </Grid.RowDefinitions>
          <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="9*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="4*"/>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="4*"/>
          </Grid.ColumnDefinitions>
          <Viewbox Grid.Column="1" Grid.ColumnSpan="2" Grid.Row="1">
            <StackPanel x:Name="MessageTitleGroup" Height="100">
              <StackPanel Orientation="Horizontal">
                <Rectangle Width="3" Height="3" Fill="#E53D434A"/>
                <Rectangle Width="400" Height="1" Fill="#343D434A" VerticalAlignment="Bottom"/>
              </StackPanel>
              <Grid Margin="10,30,0,-34">
                <TextBlock x:Name="MessageTitleShadow" Text="WARNING" Foreground="#1A242B33" FontSize="50" FontFamily="{StaticResource Font.Default.Bold}"/>
                <TextBlock x:Name="MessageTitle" Text="WARNING" Foreground="#FF242B33" FontSize="50" FontFamily="{StaticResource Font.Default.Bold}">
                <noesis:Element.Transform3D>
                  <noesis:CompositeTransform3D TranslateZ="15"/>
                </noesis:Element.Transform3D>
                </TextBlock>
              </Grid>
            </StackPanel>
          </Viewbox>
          <Viewbox Grid.Column="4" Grid.Row="1">
            <StackPanel Height="{Binding ActualHeight, ElementName=MessageTitleGroup}">
              <StackPanel Orientation="Horizontal">
                <Rectangle Width="3" Height="3" Fill="#E53D434A"/>
                <Rectangle Width="158" Height="1" Fill="#343D434A" VerticalAlignment="Bottom"/>
              </StackPanel>
            </StackPanel>
          </Viewbox>
          <Viewbox Grid.Row="3" Grid.Column="2" Grid.ColumnSpan="4">
            <StackPanel Width="300" Height="60">
              <TextBlock Text="THIS ENTRY IS NOT AVAILABLE." FontFamily="{StaticResource Font.Default.Normal}" TextWrapping="Wrap"/>
              <TextBlock Text="PLEASE TRY ANOTHER ONE." FontFamily="{StaticResource Font.Default.Normal}" TextWrapping="Wrap" Margin="20,0,0,0"/>
            </StackPanel>
          </Viewbox>
          <Viewbox Grid.Row="4" Grid.Column="5" Grid.ColumnSpan="2">
            <Button x:Name="MsgOkBtn" Style="{StaticResource Style.Button.Default}" Content="OK" HorizontalContentAlignment="Center">
              <i:Interaction.Triggers>
                <i:EventTrigger EventName="Click">
                  <ei:GoToStateAction StateName="Home"/>
                </i:EventTrigger>
                <ei:DataTrigger Binding="{Binding IsEnabled, ElementName=MsgOkBtn}" Value="True">
                  <noesis:SetFocusAction/>
                </ei:DataTrigger>
              </i:Interaction.Triggers>
            </Button>
          </Viewbox>
        </Grid>
      </Grid>
    </Grid>
    <Rectangle x:Name="BgWhite" Grid.RowSpan="4" Fill="#E6FFFFFF" IsHitTestVisible="False" d:IsLocked="True"/>
  </Grid>
</Window>
