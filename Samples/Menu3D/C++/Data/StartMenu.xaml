    <b:Interaction.Triggers>
        <b:EventTrigger EventName="Checked" SourceName="Casual">
            <b:ControlStoryboardAction Storyboard="{StaticResource ActivateCasual}"/>
        </b:EventTrigger>
        <b:EventTrigger EventName="Checked" SourceName="Normal">
            <b:ControlStoryboardAction Storyboard="{StaticResource ActivateNormal}"/>
        </b:EventTrigger>
        <b:EventTrigger EventName="Checked" SourceName="Expert">
            <b:ControlStoryboardAction Storyboard="{StaticResource ActivateExpert}"/>
        </b:EventTrigger>
        <b:DataTrigger Binding="{Binding State}" Value="{x:Static local:State.Start}">
            <b:ChangePropertyAction TargetName="Casual" PropertyName="MinHeight" Value="1"/>
            <b:ControlStoryboardAction Storyboard="{StaticResource FadeIn}"/>
        </b:DataTrigger>
        <b:StoryboardCompletedTrigger Storyboard="{StaticResource FadeIn}">
            <noesis:SetFocusAction TargetName="Casual"/>
            <b:ChangePropertyAction TargetName="Casual" PropertyName="MinHeight" Value="0"/>
        </b:StoryboardCompletedTrigger>
        <b:DataTrigger Binding="{Binding State}" Comparison="NotEqual" Value="{x:Static local:State.Start}">
            <b:ControlStoryboardAction Storyboard="{StaticResource FadeOut}"/>
        </b:DataTrigger>
        <b:KeyTrigger Key="Esc" ActiveOnFocus="True">
            <b:PlaySoundAction Source="Sounds/WaterDropBig.mp3" Volume="0.5"/>
            <b:InvokeCommandAction Command="{Binding Back}"/>
        </b:KeyTrigger>
    </b:Interaction.Triggers>

