                        <GradientStop Color="#FF347E7D" Offset="0.185"/>
                        <GradientStop Color="#7F1E5258" Offset="0.441"/>
                        <GradientStop Color="#6699F0ED" Offset="0.735"/>
                        <GradientStop Color="#FF347E7D" Offset="1"/>
