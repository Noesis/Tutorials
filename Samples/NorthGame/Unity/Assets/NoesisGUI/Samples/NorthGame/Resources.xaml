<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
                    xmlns:local="clr-namespace:NorthGame"
                    xmlns:noesis="clr-namespace:NoesisGUIExtensions;assembly=Noesis.GUI.Extensions">

    <local:UppercaseConverter x:Key="Converter.Uppercase"/>

    <SolidColorBrush x:Key="Brush.Text.LightGreen" Color="#FFCFFBF5"/>
    <SolidColorBrush x:Key="Brush.Text.MediumGreen" Color="#E58FBCB6"/>
    <SolidColorBrush x:Key="Brush.Text.DarkCyan" Color="#FF41B3C0"/>
    <SolidColorBrush x:Key="Brush.Text.ButtonNormal" Color="#FFADF8F7"/>
    <SolidColorBrush x:Key="Brush.Text.Shadow" Color="#20000000"/>

    <SolidColorBrush x:Key="Brush.Button.Border" Color="#FF1F4147"/>
    <LinearGradientBrush x:Key="Brush.Button.Background" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#FF0B191E" Offset="1"/>
        <GradientStop Color="#FF12252B" Offset="0"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="Brush.Progress.Wins" Color="#FF245359"/>
    <LinearGradientBrush x:Key="Brush.Progress.WinsShine" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#00245359" Offset="0"/>
        <GradientStop Color="#82316D79" Offset="0.19"/>
        <GradientStop Color="#6C4799AE" Offset="0.346"/>
        <GradientStop Color="#8C316679" Offset="0.566"/>
        <GradientStop Color="#7F4799AE" Offset="0.75"/>
        <GradientStop Color="#4C316D79" Offset="0.846"/>
        <GradientStop Color="#00245359" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="Brush.Progress.Initiative" Color="#FFAF6330"/>
    <LinearGradientBrush x:Key="Brush.Progress.InitiativeShine" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="#00AF6330" Offset="0"/>
        <GradientStop Color="#B2FDB34F" Offset="0.19"/>
        <GradientStop Color="#CCFBD658" Offset="0.346"/>
        <GradientStop Color="#CCFE9245" Offset="0.566"/>
        <GradientStop Color="#7FFBCF56" Offset="0.75"/>
        <GradientStop Color="#4CFD9B47" Offset="0.846"/>
        <GradientStop Color="#00AF6330" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="Brush.Icon.Background" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="#FF14FFED" Offset="0"/>
        <GradientStop Color="#FF005569" Offset="1"/>
    </LinearGradientBrush>
    
    <Style TargetType="ToolTip">
      <Setter Property="Padding" Value="20,10"/>
      <Setter Property="Template">
        <Setter.Value>
          <ControlTemplate TargetType="ToolTip">
            <Border Background="#80000000" Padding="{TemplateBinding Padding}">
              <ContentPresenter/>
            </Border>
          </ControlTemplate>
        </Setter.Value>
      </Setter>
    </Style>

    <Style x:Key="Style.Text.StatLabel" TargetType="{x:Type TextBlock}">
        <Setter Property="Width" Value="250"/>
        <Setter Property="Foreground" Value="{StaticResource Brush.Text.LightGreen}"/>
        <Setter Property="FontSize" Value="24"/>
        <Setter Property="noesis:Text.Stroke" Value="{StaticResource Brush.Text.Shadow}"/>
        <Setter Property="noesis:Text.StrokeThickness" Value="3.5"/>
    </Style>

    <Style x:Key="Style.Text.StatDesc" TargetType="{x:Type TextBlock}">
        <Setter Property="Foreground" Value="{StaticResource Brush.Text.MediumGreen}"/>
        <Setter Property="FontSize" Value="12"/>
        <Setter Property="Margin" Value="0,0,0,10"/>
    </Style>

    <Style x:Key="Style.Text.Stat" TargetType="{x:Type TextBlock}">
        <Setter Property="FontFamily" Value="Fonts/#Avenir LT Std"/>
        <Setter Property="Foreground" Value="{StaticResource Brush.Text.DarkCyan}"/>
        <Setter Property="FontSize" Value="24"/>
        <Setter Property="noesis:Text.Stroke" Value="{StaticResource Brush.Text.Shadow}"/>
        <Setter Property="noesis:Text.StrokeThickness" Value="3.5"/>
    </Style>

    <ControlTemplate x:Key="Template.Button.PrevNext" TargetType="{x:Type Button}">
        <Grid Background="Transparent">
            <Viewbox>
                <ContentPresenter Margin="{TemplateBinding Padding}"
                    noesis:Text.Stroke="{StaticResource Brush.Text.Shadow}" noesis:Text.StrokeThickness="3.5"/>
            </Viewbox>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsKeyboardFocused" Value="True">
                <Setter Property="Foreground" Value="#FF1BF4F7"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="Style.Button.PrevNext" TargetType="{x:Type Button}">
        <Setter Property="FontFamily" Value="Fonts/#Avenir LT Std"/>
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template" Value="{StaticResource Template.Button.PrevNext}"/>
    </Style>

    <GradientStopCollection x:Key="Gradient.Hover">
        <GradientStop Color="#19A5CCCF" Offset="0"/>
        <GradientStop Color="#1941999C" Offset="1"/>
    </GradientStopCollection>
    <GradientStopCollection x:Key="Gradient.Focus">
        <GradientStop Color="#0C3E2310" Offset="0"/>
        <GradientStop Color="#3FE56A1F" Offset="0.077"/>
        <GradientStop Color="#D8F1ECD5" Offset="0.118"/>
        <GradientStop Color="#8DE56A1F" Offset="0.151"/>
        <GradientStop Color="#26C96729" Offset="0.195"/>
        <GradientStop Color="#003E2310" Offset="0.264"/>
        <GradientStop Color="#003E2310" Offset="0.788"/>
        <GradientStop Color="#19C7741A" Offset="0.832"/>
        <GradientStop Color="#99E5821F" Offset="0.879"/>
        <GradientStop Color="#BFEBD27C" Offset="0.901"/>
        <GradientStop Color="#F2F1E9D5" Offset="0.922"/>
        <GradientStop Color="#A5E58B1F" Offset="0.954"/>
        <GradientStop Color="#0C3E2310" Offset="1"/>
    </GradientStopCollection>
    <LinearGradientBrush x:Key="Brush.FocusMask" StartPoint="0,0" EndPoint="1,0">
        <GradientStop Color="#0CFFFFFF" Offset="0.1"/>
        <GradientStop Color="#CCFFFFFF" Offset="0.5"/>
        <GradientStop Color="#0CFFFFFF" Offset="0.9"/>
    </LinearGradientBrush>
    <ControlTemplate x:Key="Template.Button.Menu" TargetType="{x:Type Button}">
        <Grid x:Name="Root" RenderTransformOrigin="0.5,0.5">
            <Grid.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Grid.RenderTransform>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualStateGroup.Transitions>
                        <VisualTransition GeneratedDuration="0:0:0.1" To="MouseOver">
                            <VisualTransition.GeneratedEasingFunction>
                                <SineEase EasingMode="EaseInOut"/>
                            </VisualTransition.GeneratedEasingFunction>
                        </VisualTransition>
                        <VisualTransition From="MouseOver" GeneratedDuration="0:0:0.2">
                            <VisualTransition.GeneratedEasingFunction>
                                <SineEase EasingMode="EaseInOut"/>
                            </VisualTransition.GeneratedEasingFunction>
                        </VisualTransition>
                        <VisualTransition GeneratedDuration="0:0:0.1" To="Pressed">
                            <VisualTransition.GeneratedEasingFunction>
                                <SineEase EasingMode="EaseInOut"/>
                            </VisualTransition.GeneratedEasingFunction>
                        </VisualTransition>
                    </VisualStateGroup.Transitions>
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="Hover">
                                <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
                            </DoubleAnimationUsingKeyFrames>
                            <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(TextElement.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="Text">
                                <EasingColorKeyFrame KeyTime="0" Value="#FFECFEFF"/>
                            </ColorAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="Hover">
                                <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
                            </DoubleAnimationUsingKeyFrames>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)" Storyboard.TargetName="Root">
                                <EasingDoubleKeyFrame KeyTime="0" Value="0.97"/>
                            </DoubleAnimationUsingKeyFrames>
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)" Storyboard.TargetName="Root">
                                <EasingDoubleKeyFrame KeyTime="0" Value="0.97"/>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled"/>
                </VisualStateGroup>
                <VisualStateGroup x:Name="FocusStates">
                    <VisualStateGroup.Transitions>
                        <VisualTransition GeneratedDuration="0:0:0.2" To="Focused">
                            <VisualTransition.GeneratedEasingFunction>
                                <CubicEase EasingMode="EaseOut"/>
                            </VisualTransition.GeneratedEasingFunction>
                        </VisualTransition>
                        <VisualTransition From="Focused" GeneratedDuration="0:0:0.3">
                            <VisualTransition.GeneratedEasingFunction>
                                <SineEase EasingMode="EaseInOut"/>
                            </VisualTransition.GeneratedEasingFunction>
                        </VisualTransition>
                    </VisualStateGroup.Transitions>
                    <VisualState x:Name="Unfocused"/>
                    <VisualState x:Name="Focused">
                        <Storyboard AutoReverse="True" RepeatBehavior="Forever">
                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="Focus">
                                <EasingDoubleKeyFrame KeyTime="0" Value="1">
                                    <EasingDoubleKeyFrame.EasingFunction>
                                        <SineEase EasingMode="EaseInOut"/>
                                    </EasingDoubleKeyFrame.EasingFunction>
                                </EasingDoubleKeyFrame>
                                <EasingDoubleKeyFrame KeyTime="0:0:1.1" Value="0.5">
                                    <EasingDoubleKeyFrame.EasingFunction>
                                        <SineEase EasingMode="EaseInOut"/>
                                    </EasingDoubleKeyFrame.EasingFunction>
                                </EasingDoubleKeyFrame>
                                <EasingDoubleKeyFrame KeyTime="0:0:2" Value="1">
                                    <EasingDoubleKeyFrame.EasingFunction>
                                        <SineEase EasingMode="EaseInOut"/>
                                    </EasingDoubleKeyFrame.EasingFunction>
                                </EasingDoubleKeyFrame>
                            </DoubleAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Image Source="Images/MenuButton.png" Grid.RowSpan="3" IsHitTestVisible="False"/>
            <Viewbox>
                <Grid Margin="0,10,0,12">
                    <Rectangle x:Name="Hover" Width="114" Margin="1,0,2,0">
                        <Rectangle.Fill>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" Opacity="0">
                                <GradientStop Color="#19A5CCCF" Offset="0"/>
                                <GradientStop Color="#1941999C" Offset="1"/>
                            </LinearGradientBrush>
                        </Rectangle.Fill>
                    </Rectangle>
                    <Rectangle x:Name="Focus" Width="114" Margin="0,-4" OpacityMask="{StaticResource Brush.FocusMask}">
                        <Rectangle.Fill>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0" Opacity="0" GradientStops="{StaticResource Gradient.Focus}"/>
                        </Rectangle.Fill>
                    </Rectangle>
                    <TextBlock x:Name="Text" Text="{TemplateBinding Content}" Foreground="#FFA5CCCF"
                               HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,4"
                               noesis:Text.Stroke="{StaticResource Brush.Text.Shadow}" noesis:Text.StrokeThickness="1.5"/>
                </Grid>
            </Viewbox>
        </Grid>
    </ControlTemplate>
    <Style x:Key="Style.Button.Menu" TargetType="{x:Type Button}">
        <Setter Property="Margin" Value="0,0,0,36"/>
        <Setter Property="Padding" Value="48,4"/>
        <Setter Property="FontFamily" Value="Fonts/#Avenir LT Std"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template" Value="{StaticResource Template.Button.Menu}"/>
        <Setter Property="RenderTransformOrigin" Value="0.5,0.5"/>
        <Setter Property="RenderTransform">
            <Setter.Value>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Setter.Value>
        </Setter>
    </Style>

    <ControlTemplate x:Key="Template.Button.BackMenu" TargetType="{x:Type Button}">
        <ControlTemplate.Resources>
            <Storyboard x:Key="HoverOn">
                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[0].(GradientStop.Color)" Storyboard.TargetName="Bg">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF1E5258">
                        <EasingColorKeyFrame.EasingFunction>
                            <SineEase EasingMode="EaseInOut"/>
                        </EasingColorKeyFrame.EasingFunction>
                    </EasingColorKeyFrame>
                </ColorAnimationUsingKeyFrames>
                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[1].(GradientStop.Color)" Storyboard.TargetName="Bg">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF153D41">
                        <EasingColorKeyFrame.EasingFunction>
                            <SineEase EasingMode="EaseInOut"/>
                        </EasingColorKeyFrame.EasingFunction>
                    </EasingColorKeyFrame>
                </ColorAnimationUsingKeyFrames>
                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Stroke).(GradientBrush.GradientStops)[1].(GradientStop.Color)" Storyboard.TargetName="Bd">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#FF99F0ED"/>
                </ColorAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Stroke).(GradientBrush.GradientStops)[1].(GradientStop.Offset)" Storyboard.TargetName="Bd">
                    <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="0.441"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:0.6" Value="0.734"/>
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>
            <Storyboard x:Key="HoverOff">
                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[0].(GradientStop.Color)" Storyboard.TargetName="Bg">
                    <EasingColorKeyFrame KeyTime="0:0:0.3" Value="#FF1C3A44"/>
                </ColorAnimationUsingKeyFrames>
                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[1].(GradientStop.Color)" Storyboard.TargetName="Bg">
                    <EasingColorKeyFrame KeyTime="0:0:0.3" Value="#FF122831"/>
                </ColorAnimationUsingKeyFrames>
                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Stroke).(GradientBrush.GradientStops)[1].(GradientStop.Color)" Storyboard.TargetName="Bd">
                    <EasingColorKeyFrame KeyTime="0:0:0.3" Value="#7F1E5258"/>
                </ColorAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Stroke).(GradientBrush.GradientStops)[1].(GradientStop.Offset)" Storyboard.TargetName="Bd">
                    <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="0.441"/>
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>
        </ControlTemplate.Resources>
        <Grid Margin="0,-2,0,0">
            <Path x:Name="Bg" Stroke="#FF55A6A5" StrokeThickness="2" Data="M0.5,0.5 L301.5,0.5 301.5,119.5 56.5,119.5 z" Stretch="Fill">
                <Path.Fill>
                    <LinearGradientBrush EndPoint="0.5,0" StartPoint="0.5,1">
                        <GradientStop Color="#FF1C3A44" Offset="0"/>
                        <GradientStop Color="#FF122831" Offset="1"/>
                    </LinearGradientBrush>
                </Path.Fill>
            </Path>
            <Path x:Name="Bd" Data="M0.5,0.5 L301.5,0.5 301.5,119.5 56.5,119.5 z" StrokeThickness="2" Stretch="Fill">
                <Path.Stroke>
                    <LinearGradientBrush EndPoint="1,0.501" StartPoint="0,0.501">
                        <GradientStop Color="#FF55A6A5" Offset="0.185"/>
                        <GradientStop Color="#7F1E5258" Offset="0.441"/>
                        <GradientStop Color="#FF99F0ED" Offset="0.735"/>
                        <GradientStop Color="#FF55A6A5" Offset="1"/>
                    </LinearGradientBrush>
                </Path.Stroke>
            </Path>
            <ContentPresenter Margin="{TemplateBinding Padding}"
                HorizontalAlignment="Center" VerticalAlignment="Center"
                noesis:Text.Stroke="{StaticResource Brush.Text.Shadow}" noesis:Text.StrokeThickness="3.5"/>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Trigger.ExitActions>
                    <BeginStoryboard Storyboard="{StaticResource HoverOff}"/>
                </Trigger.ExitActions>
                <Trigger.EnterActions>
                    <BeginStoryboard Storyboard="{StaticResource HoverOn}"/>
                </Trigger.EnterActions>
            </Trigger>
            <Trigger Property="IsKeyboardFocused" Value="True">
                <Setter Property="Foreground" Value="#FF1BF4F7"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="Style.Button.BackMenu" TargetType="{x:Type Button}">
        <Setter Property="Padding" Value="200,25,120,15"/>
        <Setter Property="FontFamily" Value="Fonts/#Interstate-Bold"/>
        <Setter Property="Foreground" Value="White"/>
        <Setter Property="FontSize" Value="50"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template" Value="{StaticResource Template.Button.BackMenu}"/>
    </Style>

    <ControlTemplate x:Key="Template.ListBox.HorizontalList" TargetType="{x:Type ListBox}">
        <ControlTemplate.Resources>
            <Storyboard x:Key="FocusOn">
                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)" Storyboard.TargetName="Bd">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#80D4FBF5">
                        <EasingColorKeyFrame.EasingFunction>
                            <SineEase EasingMode="EaseInOut"/>
                        </EasingColorKeyFrame.EasingFunction>
                    </EasingColorKeyFrame>
                </ColorAnimationUsingKeyFrames>
            </Storyboard>
            <Storyboard x:Key="FocusOff">
                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)" Storyboard.TargetName="Bd">
                    <EasingColorKeyFrame KeyTime="0:0:0.3" Value="#00D4FBF5">
                        <EasingColorKeyFrame.EasingFunction>
                            <SineEase EasingMode="EaseInOut"/>
                        </EasingColorKeyFrame.EasingFunction>
                    </EasingColorKeyFrame>
                </ColorAnimationUsingKeyFrames>
            </Storyboard>
        </ControlTemplate.Resources>
        <Border x:Name="Bd" CornerRadius="2" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="#00D4FBF5" Padding="{TemplateBinding Padding}">
            <StackPanel IsItemsHost="True" Orientation="Horizontal"/>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsKeyboardFocusWithin" Value="True">
                <Trigger.ExitActions>
                    <BeginStoryboard Storyboard="{StaticResource FocusOff}"/>
                </Trigger.ExitActions>
                <Trigger.EnterActions>
                    <BeginStoryboard Storyboard="{StaticResource FocusOn}"/>
                </Trigger.EnterActions>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="Style.ListBox.HorizontalList" TargetType="{x:Type ListBox}">
        <Setter Property="SelectionMode" Value="Single"/>
        <Setter Property="KeyboardNavigation.DirectionalNavigation" Value="Continue"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template" Value="{StaticResource Template.ListBox.HorizontalList}"/>
    </Style>

    <ControlTemplate x:Key="Template.ListBoxItem.Navigation" TargetType="{x:Type ListBoxItem}">
        <ControlTemplate.Resources>
            <Storyboard x:Key="HoverOn">
                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(TextElement.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="Hover">
                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="White">
                        <EasingColorKeyFrame.EasingFunction>
                            <SineEase EasingMode="EaseInOut"/>
                        </EasingColorKeyFrame.EasingFunction>
                    </EasingColorKeyFrame>
                </ColorAnimationUsingKeyFrames>
            </Storyboard>
            <Storyboard x:Key="HoverOff">
                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(TextElement.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="Hover">
                    <EasingColorKeyFrame KeyTime="0:0:0.3" Value="#FFB2E7E3">
                        <EasingColorKeyFrame.EasingFunction>
                            <SineEase EasingMode="EaseInOut"/>
                        </EasingColorKeyFrame.EasingFunction>
                    </EasingColorKeyFrame>
                </ColorAnimationUsingKeyFrames>
            </Storyboard>
            <Storyboard x:Key="FocusOn">
                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="Focus">
                    <EasingDoubleKeyFrame KeyTime="0:0:0.1" Value="1">
                        <EasingDoubleKeyFrame.EasingFunction>
                            <SineEase EasingMode="EaseInOut"/>
                        </EasingDoubleKeyFrame.EasingFunction>
                    </EasingDoubleKeyFrame>
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>
            <Storyboard x:Key="FocusOff">
                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(Brush.Opacity)" Storyboard.TargetName="Focus">
                    <EasingDoubleKeyFrame KeyTime="0:0:0.3" Value="0">
                        <EasingDoubleKeyFrame.EasingFunction>
                            <SineEase EasingMode="EaseInOut"/>
                        </EasingDoubleKeyFrame.EasingFunction>
                    </EasingDoubleKeyFrame>
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>
            <Storyboard x:Key="SelectOn">
                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(TextElement.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="Select">
                    <EasingColorKeyFrame KeyTime="0" Value="#FFB2E7E3"/>
                    <EasingColorKeyFrame KeyTime="0:0:0.1" Value="#FF1BF4F7">
                        <EasingColorKeyFrame.EasingFunction>
                            <SineEase EasingMode="EaseInOut"/>
                        </EasingColorKeyFrame.EasingFunction>
                    </EasingColorKeyFrame>
                </ColorAnimationUsingKeyFrames>
            </Storyboard>
            <Storyboard x:Key="SelectOff">
                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(TextElement.Foreground).(SolidColorBrush.Color)" Storyboard.TargetName="Select">
                    <EasingColorKeyFrame KeyTime="0:0:0.3" Value="#00B2E7E3">
                        <EasingColorKeyFrame.EasingFunction>
                            <SineEase EasingMode="EaseInOut"/>
                        </EasingColorKeyFrame.EasingFunction>
                    </EasingColorKeyFrame>
                </ColorAnimationUsingKeyFrames>
            </Storyboard>
        </ControlTemplate.Resources>
        <Grid Background="Transparent">
            <Grid VerticalAlignment="Center">
                <Ellipse x:Name="Focus" Height="16" VerticalAlignment="Bottom" Margin="-16,0">
                    <Ellipse.Fill>
                        <RadialGradientBrush Opacity="0">
                            <GradientStop Color="#40B2E7E3" Offset="0"/>
                            <GradientStop Color="#00B2E7E3" Offset="1"/>
                        </RadialGradientBrush>
                    </Ellipse.Fill>
                </Ellipse>
                <TextBlock x:Name="Hover" Text="{Binding Content, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource Converter.Uppercase}}"
                            HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#FFB2E7E3"
                            noesis:Text.Stroke="{StaticResource Brush.Text.Shadow}" noesis:Text.StrokeThickness="3.5"/>
                <TextBlock x:Name="Select" Text="{Binding Content, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource Converter.Uppercase}}"
                            HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#00B2E7E3"
                            noesis:Text.Stroke="{StaticResource Brush.Text.Shadow}" noesis:Text.StrokeThickness="3.5"/>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Trigger.ExitActions>
                    <BeginStoryboard Storyboard="{StaticResource HoverOff}"/>
                </Trigger.ExitActions>
                <Trigger.EnterActions>
                    <BeginStoryboard Storyboard="{StaticResource HoverOn}"/>
                </Trigger.EnterActions>
            </Trigger>
            <Trigger Property="IsKeyboardFocused" Value="True">
                <Trigger.ExitActions>
                    <BeginStoryboard Storyboard="{StaticResource FocusOff}"/>
                </Trigger.ExitActions>
                <Trigger.EnterActions>
                    <BeginStoryboard Storyboard="{StaticResource FocusOn}"/>
                </Trigger.EnterActions>
            </Trigger>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="Visibility" Value="Collapsed" TargetName="Hover"/>
                <Trigger.ExitActions>
                    <BeginStoryboard Storyboard="{StaticResource SelectOff}"/>
                </Trigger.ExitActions>
                <Trigger.EnterActions>
                    <BeginStoryboard Storyboard="{StaticResource SelectOn}"/>
                </Trigger.EnterActions>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="Style.ListBoxItem.Navigation" TargetType="{x:Type ListBoxItem}">
        <Setter Property="Margin" Value="80,12,80,0"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="FontSize" Value="28"/>
        <Setter Property="FontFamily" Value="Fonts/#Avenir LT Std"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template" Value="{StaticResource Template.ListBoxItem.Navigation}"/>
    </Style>

    <ControlTemplate x:Key="Template.ScrollViewer.TextBox" TargetType="{x:Type ScrollViewer}">
        <ScrollContentPresenter Margin="{TemplateBinding Padding}"/>
    </ControlTemplate>
