                <Setter Property="Visibility" Value="Visible" TargetName="RaidArena"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Type}" Value="Explore">
                <Setter Property="Visibility" Value="Visible" TargetName="RaidExplore"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Type}" Value="Defend">
                <Setter Property="Visibility" Value="Visible" TargetName="RaidDefend"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Difficulty}" Value="Easy">
                <Setter Property="Visibility" Value="Visible" TargetName="DifficultyEasy"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Difficulty}" Value="Normal">
                <Setter Property="Visibility" Value="Visible" TargetName="DifficultyNormal"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding Difficulty}" Value="Hard">
                <Setter Property="Visibility" Value="Visible" TargetName="DifficultyHard"/>
            </DataTrigger>
        </ControlTemplate.Triggers>
