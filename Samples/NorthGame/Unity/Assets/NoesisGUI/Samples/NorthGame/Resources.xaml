                    <LinearGradientBrush StartPoint="0,0" EndPoint="0,1">
                        <GradientStop Color="#FF132932" Offset="0"/>
                        <GradientStop Color="#FF1C3A45" Offset="1"/>
                    </LinearGradientBrush>
                </Border.Background>
            </Border>
            <Border x:Name="BgOver" BorderThickness="1">
                <Border.Background>
                    <LinearGradientBrush StartPoint="0,0" EndPoint="0,1" Opacity="0">
                        <GradientStop Color="#264584A0" Offset="0"/>
                        <GradientStop Color="#264288A4" Offset="1"/>
