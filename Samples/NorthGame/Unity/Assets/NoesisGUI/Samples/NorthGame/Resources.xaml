                <Track.DecreaseRepeatButton>
                    <RepeatButton Style="{StaticResource Style.RepeatButton.ScrollBar}" Command="ScrollBar.PageUpCommand"/>
                </Track.DecreaseRepeatButton>
                <Track.Thumb>
                    <Thumb x:Name="Thumb" Style="{StaticResource Style.Thumb.ScrollBar}"/>
