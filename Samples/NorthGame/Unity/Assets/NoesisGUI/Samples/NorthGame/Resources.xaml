    <Style x:Key="Style.ListBoxItem.Location" TargetType="{x:Type ListBoxItem}">
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="Template" Value="{StaticResource Template.ListBoxItem.Location}"/>
    </Style>

    <ControlTemplate x:Key="Template.ProgressBar" TargetType="{x:Type ProgressBar}">
        <ControlTemplate.Resources>
            <Storyboard x:Key="ShineLoop" Duration="0:0:8" RepeatBehavior="Forever">
                <DoubleAnimationUsingKeyFrames BeginTime="0:0:2" Storyboard.TargetProperty="(FrameworkElement.LayoutTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)" Storyboard.TargetName="ShineMove">
                    <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:3" Value="0.95">
                        <EasingDoubleKeyFrame.EasingFunction>
                            <SineEase EasingMode="EaseOut"/>
                        </EasingDoubleKeyFrame.EasingFunction>
                    </EasingDoubleKeyFrame>
                    <EasingDoubleKeyFrame KeyTime="0:0:5" Value="1">
                        <EasingDoubleKeyFrame.EasingFunction>
                            <CircleEase EasingMode="EaseOut"/>
                        </EasingDoubleKeyFrame.EasingFunction>
                    </EasingDoubleKeyFrame>
                </DoubleAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames BeginTime="0:0:2" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="Shine">
                    <EasingDoubleKeyFrame KeyTime="0" Value="0"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1" Value="1"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:3" Value="1"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:5" Value="0"/>
                </DoubleAnimationUsingKeyFrames>
                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)" Storyboard.TargetName="Shine">
                    <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:1" Value="1"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:3" Value="1"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:4" Value="1.2"/>
                    <EasingDoubleKeyFrame KeyTime="0:0:5" Value="1.1"/>
                </DoubleAnimationUsingKeyFrames>
            </Storyboard>
        </ControlTemplate.Resources>
        <Grid>
            <Border x:Name="PART_Track"
                    Background="#7F000000"
                    BorderBrush="{TemplateBinding Background}"
                    BorderThickness="0.3"/>
            <Grid x:Name="PART_Indicator" HorizontalAlignment="Left">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="6*"/>
                    <ColumnDefinition Width="4*"/>
                </Grid.ColumnDefinitions>
                <Border Grid.ColumnSpan="2" Background="{TemplateBinding Background}"/>
                <Decorator x:Name="ShineMoveWidth" Grid.Column="0"/>
                <Decorator x:Name="ShineWidth" Grid.Column="1"/>
                <StackPanel Grid.ColumnSpan="2" Orientation="Horizontal">
                    <Decorator x:Name="ShineMove" Width="{Binding ActualWidth, ElementName=ShineMoveWidth}">
                        <Decorator.LayoutTransform>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform/>
                                <RotateTransform/>
                                <TranslateTransform/>
                            </TransformGroup>
                        </Decorator.LayoutTransform>
                    </Decorator>
                    <Border x:Name="Shine" Margin="0,0,-5,0" Opacity="0"
                            Width="{Binding ActualWidth, ElementName=ShineWidth}"
                            Background="{TemplateBinding BorderBrush}" RenderTransformOrigin="0.5,0.5">
                        <Border.RenderTransform>
                            <TransformGroup>
                                <ScaleTransform/>
                                <SkewTransform/>
                                <RotateTransform/>
                                <TranslateTransform/>
                            </TransformGroup>
                        </Border.RenderTransform>
                    </Border>
                </StackPanel>
            </Grid>
        </Grid>
        <ControlTemplate.Triggers>
            <EventTrigger RoutedEvent="FrameworkElement.Loaded">
                <BeginStoryboard Storyboard="{StaticResource ShineLoop}"/>
            </EventTrigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style TargetType="{x:Type ProgressBar}">
        <Setter Property="OverridesDefaultStyle" Value="True"/>
        <Setter Property="Background" Value="White"/>
        <Setter Property="BorderBrush" Value="White"/>
        <Setter Property="Template" Value="{StaticResource Template.ProgressBar}"/>
    </Style>
</ResourceDictionary>