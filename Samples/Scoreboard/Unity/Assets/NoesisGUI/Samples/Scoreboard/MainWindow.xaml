            <Style x:Key="PlayersHeaderStyle" TargetType="{x:Type TextBlock}">
                <Setter Property="FontSize" Value="20"/>
                <Setter Property="HorizontalAlignment" Value="Center"/>
                <Setter Property="VerticalAlignment" Value="Center"/>
                <Setter Property="Foreground" Value="{StaticResource PlayersHeaderFg}"/>
            </Style>
            <Style x:Key="ButtonStyle" TargetType="{x:Type Button}">
                <Setter Property="Foreground" Value="{StaticResource ScoreFg}"/>
                <Setter Property="FontSize" Value="24"/>
            </Style>
            <DataTemplate x:Key="PlayerTemplate">
                <Grid x:Name="PlayerRow">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="8*"/>
                        <ColumnDefinition Width="25*"/>
                        <ColumnDefinition Width="15*"/>
                        <ColumnDefinition Width="12*"/>
                        <ColumnDefinition Width="12*"/>
                        <ColumnDefinition Width="14*"/>
                        <ColumnDefinition Width="14*"/>
                    </Grid.ColumnDefinitions>
                    <Border x:Name="BackgroundBorder" Grid.ColumnSpan="7" BorderThickness="0,1,0,1" CornerRadius="1" Margin="0,1"/>
                    <Border Grid.Column="2" Background="{StaticResource HighlighColumnBg}"/>
                    <Path x:Name="ClassIcon" Fill="{StaticResource PlayersHeaderFg}" Stretch="Uniform" Margin="8" Width="21"/>
                    <TextBlock x:Name="PlayerName" Text="{Binding Name}" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="20" Grid.Column="1"/>
                    <TextBlock Text="{Binding Score}" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{StaticResource ScoreFg}" FontSize="20" Grid.Column="2"/>
                    <TextBlock Text="{Binding Kills}" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{StaticResource NormalFg}" FontSize="16" Grid.Column="3"/>
                    <TextBlock Text="{Binding Deaths}" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{StaticResource NormalFg}" FontSize="16" Grid.Column="4"/>
                    <TextBlock Text="{Binding Damage, Converter={StaticResource ThousandConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{StaticResource NormalFg}" FontSize="16" Grid.Column="5"/>
                    <TextBlock Text="{Binding Heal, Converter={StaticResource ThousandConverter}}" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="{StaticResource NormalFg}" FontSize="16" Grid.Column="6"/>
                </Grid>
                <DataTemplate.Triggers>
                    <DataTrigger Binding="{Binding Team}" Value="Alliance">
                        <Setter TargetName="BackgroundBorder" Property="Background" Value="{StaticResource AllianceBg}"/>
                        <Setter TargetName="BackgroundBorder" Property="BorderBrush" Value="{StaticResource AllianceBd}"/>
                        <Setter TargetName="PlayerName" Property="Foreground" Value="{StaticResource AllianceFg}"/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding Team}" Value="Horde">
                        <Setter TargetName="BackgroundBorder" Property="Background" Value="{StaticResource HordeBg}"/>
                        <Setter TargetName="BackgroundBorder" Property="BorderBrush" Value="{StaticResource HordeBd}"/>
                        <Setter TargetName="PlayerName" Property="Foreground" Value="{StaticResource HordeFg}"/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding Class}" Value="Fighter">
                        <Setter TargetName="ClassIcon" Property="Data" Value="{StaticResource ClassFighter}"/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding Class}" Value="Rogue">
                        <Setter TargetName="ClassIcon" Property="Data" Value="{StaticResource ClassRogue}"/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding Class}" Value="Hunter">
                        <Setter TargetName="ClassIcon" Property="Data" Value="{StaticResource ClassHunter}"/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding Class}" Value="Mage">
                        <Setter TargetName="ClassIcon" Property="Data" Value="{StaticResource ClassMage}"/>
                    </DataTrigger>
                    <DataTrigger Binding="{Binding Class}" Value="Cleric">
                        <Setter TargetName="ClassIcon" Property="Data" Value="{StaticResource ClassCleric}"/>
                    </DataTrigger>
                    <MultiDataTrigger>
                        <MultiDataTrigger.Conditions>
                            <Condition Binding="{Binding SelectedItem, ElementName=VisibleTeam}" Value="Horde"/>
                            <Condition Binding="{Binding Team}" Value="Alliance"/>
                        </MultiDataTrigger.Conditions>
                        <Setter TargetName="PlayerRow" Property="Visibility" Value="Collapsed"/>
                    </MultiDataTrigger>
                    <MultiDataTrigger>
                        <MultiDataTrigger.Conditions>
                            <Condition Binding="{Binding SelectedItem, ElementName=VisibleTeam}" Value="Alliance"/>
                            <Condition Binding="{Binding Team}" Value="Horde"/>
                        </MultiDataTrigger.Conditions>
                        <Setter TargetName="PlayerRow" Property="Visibility" Value="Collapsed"/>
                    </MultiDataTrigger>
                </DataTemplate.Triggers>
            </DataTemplate>
        </ResourceDictionary>
