                </Path>
                <TextBlock Text="{Binding Name}" Grid.Column="1" Grid.ColumnSpan="3" VerticalAlignment="Center" HorizontalAlignment="Center"
                           FontFamily="Fonts/#PerryGothic" FontSize="36" Foreground="White"/>
                <Rectangle Grid.Row="1" Margin="20,8,150,20" Fill="{StaticResource HighlightScore}"/>
                <Path Data="{StaticResource EmblemAlliance}" Stretch="Uniform" Grid.Row="1" Grid.Column="0" HorizontalAlignment="Left" Margin="22,14,8,22" StrokeThickness="4" Stroke="#19000000" StrokeLineJoin="Round"/>
                <Path Data="{StaticResource EmblemAlliance}" Fill="{StaticResource AllianceColor}" Stretch="Uniform" Grid.Row="1" Grid.Column="0" HorizontalAlignment="Left" Margin="24,16,8,24"/>
                <Rectangle Grid.Row="1" Margin="146,8,20,20" Fill="{StaticResource HighlightScore}" Grid.Column="6"/>
                <Path Data="{StaticResource EmblemHorde}" Stretch="Uniform" Grid.Row="1" Grid.Column="4" HorizontalAlignment="Right" Margin="8,14,22,22" Stroke="#19000000" StrokeThickness="4" StrokeLineJoin="Round"/>
                <Path Data="{StaticResource EmblemHorde}" Fill="{DynamicResource HordeColor}" Stretch="Uniform" Grid.Row="1" Grid.Column="4" HorizontalAlignment="Right" Margin="8,16,24,24"/>
                <TextBlock Text="Alliance" Grid.Row="1" VerticalAlignment="Center" Foreground="White" FontSize="40" HorizontalAlignment="Right" Margin="0,0,0,8"/>
                <TextBlock Text="Horde" Grid.Row="1" VerticalAlignment="Center" Foreground="White" FontSize="40" HorizontalAlignment="Left" Grid.Column="4" Margin="0,0,0,8"/>
                <Rectangle Grid.Row="1" Grid.Column="1" Margin="8,18,8,26" Fill="{StaticResource HighlightScore}"/>
                <TextBlock Text="{Binding AllianceScore}" Grid.Row="1" VerticalAlignment="Center" Foreground="#FFFFDC64" FontSize="48" HorizontalAlignment="Center" Margin="8,0,0,14" Grid.Column="1"/>
                <Rectangle Grid.Row="1" Grid.Column="3" Margin="8,18,8,26" Fill="{StaticResource HighlightScore}"/>
                <TextBlock Text="{Binding HordeScore}" Grid.Row="1" VerticalAlignment="Center" Foreground="#FFFFDC64" FontSize="48" HorizontalAlignment="Center" Margin="0,0,8,14" Grid.Column="3"/>
                <Border Grid.ColumnSpan="10" Grid.Row="2" Padding="8,0">
                    <Grid Margin="0,4,16,0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="8*"/>
                            <ColumnDefinition Width="25*"/>
                            <ColumnDefinition Width="15*"/>
                            <ColumnDefinition Width="12*"/>
                            <ColumnDefinition Width="12*"/>
                            <ColumnDefinition Width="14*"/>
                            <ColumnDefinition Width="14*"/>
                        </Grid.ColumnDefinitions>
                        <TextBlock Text="Name" Style="{StaticResource PlayersHeaderStyle}" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                        <TextBlock Text="Score" Style="{StaticResource PlayersHeaderStyle}" Grid.Column="2" VerticalAlignment="Top"/>
                        <TextBlock Text="Kills" Style="{StaticResource PlayersHeaderStyle}" Grid.Column="3" VerticalAlignment="Top"/>
                        <TextBlock Text="Deaths" Style="{StaticResource PlayersHeaderStyle}" Grid.Column="4" VerticalAlignment="Top"/>
                        <TextBlock Text="Damage" Style="{StaticResource PlayersHeaderStyle}" Grid.Column="5" VerticalAlignment="Top"/>
                        <TextBlock Text="Heal" Style="{StaticResource PlayersHeaderStyle}" Grid.Column="6" VerticalAlignment="Top"/>
                    </Grid>
                </Border>
                <Border Grid.ColumnSpan="5" Grid.Row="3" BorderThickness="8,0" Background="#FF161D27">
                    <Grid>
                        <Path Data="{StaticResource EmblemGame}" Stretch="UniformToFill" Fill="#4C0D131D" VerticalAlignment="Center" Height="460" HorizontalAlignment="Center" Width="800"/>
                        <ScrollViewer>
                            <Grid>
                                <ItemsControl ItemsSource="{Binding Players}" ItemTemplate="{StaticResource PlayerTemplate}"/>
                            </Grid>
                        </ScrollViewer>
                    </Grid>
                </Border>
                <Rectangle Grid.ColumnSpan="5" Grid.Row="3" Height="16" VerticalAlignment="Top" Margin="8,0,24,0" Fill="{StaticResource ListShadow}"/>
                <Rectangle Grid.ColumnSpan="5" Grid.Row="3" Height="16" VerticalAlignment="Bottom" Margin="8,0,24,0" Fill="{StaticResource ListShadow}" RenderTransformOrigin="0.5,0.5">
                    <Rectangle.RenderTransform>
                        <ScaleTransform ScaleY="-1" ScaleX="1"/>
                    </Rectangle.RenderTransform>
                </Rectangle>
                <Grid Grid.Row="4" Grid.ColumnSpan="5" Margin="16,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="20*"/>
                        <ColumnDefinition Width="18*"/>
                        <ColumnDefinition Width="4*"/>
                        <ColumnDefinition Width="18*"/>
                        <ColumnDefinition Width="20*"/>
                        <ColumnDefinition Width="20*"/>
                    </Grid.ColumnDefinitions>
                    <ComboBox x:Name="VisibleTeam" ItemsSource="{Binding VisibleTeams}" SelectedIndex="{Binding SelectedTeam}" Foreground="{StaticResource ScoreFg}" Grid.Column="0" Width="150" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="8,0,0,0" Padding="8,4" FontSize="20"/>
                    <TextBlock Text="Elapsed time: " Grid.Column="1" Style="{StaticResource PlayersHeaderStyle}" HorizontalAlignment="Right"/>
                    <TextBlock Text="{Binding ElapsedTime}" Grid.Column="2" Style="{StaticResource PlayersHeaderStyle}" Foreground="White" HorizontalAlignment="Right"/>
                    <TextBlock Text="Minutes" Grid.Column="3" Style="{StaticResource PlayersHeaderStyle}" Foreground="White" HorizontalAlignment="Left" Margin="8,0,0,0"/>
                    <Button Content="Play Again" Grid.Column="4" Margin="8,0" VerticalAlignment="Center" FontSize="24" Foreground="{StaticResource ScoreFg}"/>
                    <Button Content="Leave" Grid.Column="5" Margin="8,0" VerticalAlignment="Center" FontSize="24" Foreground="{StaticResource ScoreFg}"/>
                </Grid>
            </Grid>
        </Viewbox>
    </Grid>
</UserControl>