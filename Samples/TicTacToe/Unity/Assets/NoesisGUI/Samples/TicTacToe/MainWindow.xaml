        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        xmlns:local="clr-namespace:TicTacToe"
        xmlns:noesis="clr-namespace:NoesisGUIExtensions;assembly=Noesis.GUI.Extensions"
        Foreground="White" FontFamily="Fonts/#Multicolore " Focusable="True"
        d:DataContext="{d:DesignInstance {x:Type local:ViewModel}, IsDesignTimeCreatable=True}">

    <UserControl.Resources>

        <!--Cell indices-->
        <sys:Int32 x:Key="Index0">0</sys:Int32>
        <sys:Int32 x:Key="Index1">1</sys:Int32>
        <sys:Int32 x:Key="Index2">2</sys:Int32>
        <sys:Int32 x:Key="Index3">3</sys:Int32>
        <sys:Int32 x:Key="Index4">4</sys:Int32>
        <sys:Int32 x:Key="Index5">5</sys:Int32>
        <sys:Int32 x:Key="Index6">6</sys:Int32>
        <sys:Int32 x:Key="Index7">7</sys:Int32>
        <sys:Int32 x:Key="Index8">8</sys:Int32>

        <!--Cell style-->
        <Style TargetType="ToggleButton">
            <Style.Triggers>
                <Trigger Property="IsChecked" Value="True">
                    <Setter Property="IsEnabled" Value="False"/>
                </Trigger>
            </Style.Triggers>
            <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="ToggleButton">
                        <ControlTemplate.Resources>
                            <Geometry x:Key="CrossGeometry">M5.9972302,8.8379496 L8.8379328,6.0009854 25.338824,22.513108 41.847229,5.9970086 44.672782,8.8377386 28.183817,25.350064 44.665872,41.83577 41.848676,44.676558 25.339185,28.160435 8.8419424,44.687937 5.9900543,41.834826 22.513228,25.334886 z</Geometry>
                            <Storyboard x:Key="OverAnimOn">
                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleX)" Storyboard.TargetName="OverGroup">
                                    <EasingDoubleKeyFrame KeyTime="0" Value="0.5"/>
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1">
                                        <EasingDoubleKeyFrame.EasingFunction>
                                            <BackEase EasingMode="EaseOut" Amplitude="0.4"/>
                                        </EasingDoubleKeyFrame.EasingFunction>
                                    </EasingDoubleKeyFrame>
                                </DoubleAnimationUsingKeyFrames>
                                <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[0].(ScaleTransform.ScaleY)" Storyboard.TargetName="OverGroup">
                                    <EasingDoubleKeyFrame KeyTime="0" Value="0.5"/>
                                    <EasingDoubleKeyFrame KeyTime="0:0:0.2" Value="1">
                                        <EasingDoubleKeyFrame.EasingFunction>
                                            <BackEase EasingMode="EaseOut" Amplitude="0.4"/>
                                        </EasingDoubleKeyFrame.EasingFunction>
                                    </EasingDoubleKeyFrame>
                                </DoubleAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Stroke).(SolidColorBrush.Color)" Storyboard.TargetName="OverNought">
                                    <EasingColorKeyFrame KeyTime="0" Value="#00FFFFFF"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#40FFFFFF">
                                        <EasingColorKeyFrame.EasingFunction>
                                            <SineEase EasingMode="EaseInOut"/>
                                        </EasingColorKeyFrame.EasingFunction>
                                    </EasingColorKeyFrame>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="OverCross">
                                    <EasingColorKeyFrame KeyTime="0" Value="#00FFFFFF"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#40FFFFFF">
                                        <EasingColorKeyFrame.EasingFunction>
                                            <SineEase EasingMode="EaseInOut"/>
                                        </EasingColorKeyFrame.EasingFunction>
                                    </EasingColorKeyFrame>
                                </ColorAnimationUsingKeyFrames>
                            </Storyboard>
                            <Storyboard x:Key="OverAnimOff">
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Stroke).(SolidColorBrush.Color)" Storyboard.TargetName="OverNought">
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#00FFFFFF">
                                        <EasingColorKeyFrame.EasingFunction>
                                            <SineEase EasingMode="EaseInOut"/>
                                        </EasingColorKeyFrame.EasingFunction>
                                    </EasingColorKeyFrame>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="OverCross">
                                    <EasingColorKeyFrame KeyTime="0:0:0.2" Value="#00FFFFFF">
                                        <EasingColorKeyFrame.EasingFunction>
                                            <SineEase EasingMode="EaseInOut"/>
                                        </EasingColorKeyFrame.EasingFunction>
                                    </EasingColorKeyFrame>
                                </ColorAnimationUsingKeyFrames>
                            </Storyboard>
                            <Storyboard x:Key="CheckAnimOn">
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Stroke).(SolidColorBrush.Color)" Storyboard.TargetName="CheckNought">
                                    <EasingColorKeyFrame KeyTime="0" Value="#40FFFFFF"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.1" Value="#FFFFFFFF">
                                        <EasingColorKeyFrame.EasingFunction>
                                            <SineEase EasingMode="EaseInOut"/>
                                        </EasingColorKeyFrame.EasingFunction>
                                    </EasingColorKeyFrame>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="CheckCross">
                                    <EasingColorKeyFrame KeyTime="0" Value="#40FFFFFF"/>
                                    <EasingColorKeyFrame KeyTime="0:0:0.1" Value="#FFFFFFFF">
                                        <EasingColorKeyFrame.EasingFunction>
                                            <SineEase EasingMode="EaseInOut"/>
                                        </EasingColorKeyFrame.EasingFunction>
                                    </EasingColorKeyFrame>
                                </ColorAnimationUsingKeyFrames>
                            </Storyboard>
                            <Storyboard x:Key="CheckAnimOff">
