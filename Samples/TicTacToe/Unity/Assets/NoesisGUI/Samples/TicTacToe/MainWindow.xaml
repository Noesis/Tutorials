                    </Grid.RowDefinitions>
                    <Rectangle x:Name="GridH1" Fill="#FF2AA6E2" Grid.ColumnSpan="3" VerticalAlignment="Bottom" Height="4" Margin="0,0,0,-2"/>
                    <Rectangle x:Name="GridH2" Fill="#FF2AA6E2" Grid.ColumnSpan="3" VerticalAlignment="Bottom" Height="4" Margin="0,0,0,-2" Grid.Row="1"/>
                    <Rectangle x:Name="GridV1" Fill="#FF2AA6E2" Grid.RowSpan="3" HorizontalAlignment="Right" Width="4" Margin="0,0,-2,0"/>
                    <Rectangle x:Name="GridV2" Fill="#FF2AA6E2" Grid.RowSpan="3" HorizontalAlignment="Right" Width="4" Margin="0,0,-2,0" Grid.Column="1"/>
                </Grid>
                <Button x:Name="Start" Visibility="Collapsed" FocusVisualStyle="{x:Null}">
                    <Button.Template>
                        <ControlTemplate TargetType="{x:Type Button}">
                            <Border Background="#00FF0000"/>
                        </ControlTemplate>
                    </Button.Template>
                    <b:Interaction.Triggers>
                        <b:DataTrigger Binding="{Binding State}" Comparison="GreaterThanOrEqual" Value="{x:Static local:State.Tie}">
                            <b:ChangePropertyAction PropertyName="Visibility" Value="Visible"/>
                            <b:ChangePropertyAction PropertyName="IsEnabled" Value="False" TargetName="Cells"/>
                            <noesis:SetFocusAction/>
                        </b:DataTrigger>
                        <b:EventTrigger EventName="Click">
                            <b:ChangePropertyAction PropertyName="Visibility" Value="Collapsed"/>
                            <b:ControlStoryboardAction Storyboard="{StaticResource BoardFlip1Anim}"/>
                        </b:EventTrigger>
                        <b:StoryboardCompletedTrigger Storyboard="{StaticResource BoardFlip1Anim}">
                            <b:ChangePropertyAction PropertyName="IsChecked" Value="False" TargetName="Cell00"/>
                            <b:ChangePropertyAction PropertyName="IsChecked" Value="False" TargetName="Cell01"/>
                            <b:ChangePropertyAction PropertyName="IsChecked" Value="False" TargetName="Cell02"/>
                            <b:ChangePropertyAction PropertyName="IsChecked" Value="False" TargetName="Cell10"/>
                            <b:ChangePropertyAction PropertyName="IsChecked" Value="False" TargetName="Cell11"/>
                            <b:ChangePropertyAction PropertyName="IsChecked" Value="False" TargetName="Cell12"/>
                            <b:ChangePropertyAction PropertyName="IsChecked" Value="False" TargetName="Cell20"/>
                            <b:ChangePropertyAction PropertyName="IsChecked" Value="False" TargetName="Cell21"/>
                            <b:ChangePropertyAction PropertyName="IsChecked" Value="False" TargetName="Cell22"/>
                            <b:ChangePropertyAction PropertyName="Content" Value="" TargetName="Cell00"/>
                            <b:ChangePropertyAction PropertyName="Content" Value="" TargetName="Cell01"/>
                            <b:ChangePropertyAction PropertyName="Content" Value="" TargetName="Cell02"/>
                            <b:ChangePropertyAction PropertyName="Content" Value="" TargetName="Cell10"/>
                            <b:ChangePropertyAction PropertyName="Content" Value="" TargetName="Cell11"/>
                            <b:ChangePropertyAction PropertyName="Content" Value="" TargetName="Cell12"/>
                            <b:ChangePropertyAction PropertyName="Content" Value="" TargetName="Cell20"/>
                            <b:ChangePropertyAction PropertyName="Content" Value="" TargetName="Cell21"/>
                            <b:ChangePropertyAction PropertyName="Content" Value="" TargetName="Cell22"/>
                            <b:ControlStoryboardAction Storyboard="{StaticResource BoardFlip2Anim}"/>
                            <b:InvokeCommandAction Command="{Binding StartCommand}"/>
                            <b:ChangePropertyAction PropertyName="IsEnabled" Value="True" TargetName="Cells"/>
                        </b:StoryboardCompletedTrigger>
                    </b:Interaction.Triggers>
                </Button>
            </Grid>
        </Viewbox>
        <Viewbox Grid.Row="2">
            <Grid Width="300" Height="100" Margin="20,0,20,20">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="3*"/>
                    <RowDefinition Height="7*"/>
                </Grid.RowDefinitions>
                <TextBlock Text="PLAYER 1" Grid.Row="0" Grid.Column="0" FontSize="20" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                <TextBlock Text="TIES" Grid.Row="0" Grid.Column="1" FontSize="20" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                <TextBlock Text="PLAYER 2" Grid.Row="0" Grid.Column="2" FontSize="20" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                <TextBlock x:Name="ScorePlayer1Fade" Text="{Binding ScorePlayer1}" Foreground="#00FFFFFF" Grid.Row="1" Grid.Column="0" FontSize="52" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
                    <TextBlock.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </TextBlock.RenderTransform>
                </TextBlock>
                <TextBlock x:Name="ScoreTiesFade" Text="{Binding ScoreTies}" Foreground="#00FFFFFF" Grid.Row="1" Grid.Column="1" FontSize="52" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
                    <TextBlock.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </TextBlock.RenderTransform>
                </TextBlock>
                <TextBlock x:Name="ScorePlayer2Fade" Text="{Binding ScorePlayer2}" Foreground="#00FFFFFF" Grid.Row="1" Grid.Column="2" FontSize="52" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
                    <TextBlock.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </TextBlock.RenderTransform>
                </TextBlock>
                <TextBlock x:Name="ScorePlayer1" Text="{Binding ScorePlayer1, FallbackValue=0}" Foreground="White" Grid.Row="1" Grid.Column="0" FontSize="52" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
                    <b:Interaction.Triggers>
                        <b:DataTrigger Binding="{Binding Text, ElementName=ScorePlayer1}" Comparison="NotEqual" Value="0">
                            <b:ControlStoryboardAction Storyboard="{StaticResource ScorePlayer1ChangedAnim}"/>
                        </b:DataTrigger>
                    </b:Interaction.Triggers>
                    <TextBlock.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </TextBlock.RenderTransform>
                </TextBlock>
                <TextBlock x:Name="ScoreTies" Text="{Binding ScoreTies, FallbackValue=0}" Foreground="White" Grid.Row="1" Grid.Column="1" FontSize="52" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
                    <b:Interaction.Triggers>
                        <b:DataTrigger Binding="{Binding Text, ElementName=ScoreTies}" Comparison="NotEqual" Value="0">
                            <b:ControlStoryboardAction Storyboard="{StaticResource ScoreTiesChangedAnim}"/>
                        </b:DataTrigger>
                    </b:Interaction.Triggers>
                    <TextBlock.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </TextBlock.RenderTransform>
                </TextBlock>
                <TextBlock x:Name="ScorePlayer2" Text="{Binding ScorePlayer2, FallbackValue=0}" Foreground="White" Grid.Row="1" Grid.Column="2" FontSize="52" HorizontalAlignment="Center" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
                    <b:Interaction.Triggers>
                        <b:DataTrigger Binding="{Binding Text, ElementName=ScorePlayer2}" Comparison="NotEqual" Value="0">
                            <b:ControlStoryboardAction Storyboard="{StaticResource ScorePlayer2ChangedAnim}"/>
                        </b:DataTrigger>
                    </b:Interaction.Triggers>
                    <TextBlock.RenderTransform>
                        <TransformGroup>
                            <ScaleTransform/>
                            <SkewTransform/>
                            <RotateTransform/>
                            <TranslateTransform/>
                        </TransformGroup>
                    </TextBlock.RenderTransform>
                </TextBlock>
            </Grid>
        </Viewbox>
    </Grid>
</UserControl>
