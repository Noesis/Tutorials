                                    <EasingColorKeyFrame KeyTime="0" Value="#00FFFFFF">
                                        <EasingColorKeyFrame.EasingFunction>
                                            <SineEase EasingMode="EaseInOut"/>
                                        </EasingColorKeyFrame.EasingFunction>
                                    </EasingColorKeyFrame>
                                </ColorAnimationUsingKeyFrames>
                                <ColorAnimationUsingKeyFrames Storyboard.TargetProperty="(Shape.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="CheckCross">
                                    <EasingColorKeyFrame KeyTime="0" Value="#00FFFFFF">
                                        <EasingColorKeyFrame.EasingFunction>
                                            <SineEase EasingMode="EaseInOut"/>
                                        </EasingColorKeyFrame.EasingFunction>
                                    </EasingColorKeyFrame>
                                </ColorAnimationUsingKeyFrames>
                            </Storyboard>
                        </ControlTemplate.Resources>
                        <Grid Background="Transparent">
                            <Grid x:Name="OverGroup" RenderTransformOrigin="0.5,0.5">
                                <Grid.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform/>
                                        <SkewTransform/>
                                        <RotateTransform/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Grid.RenderTransform>
                                <Ellipse x:Name="OverNought" Stroke="#00FFFFFF" StrokeThickness="4" Margin="15" noesis:Element.PPAAIn="0"/>
                                <Path x:Name="OverCross" Fill="#00FFFFFF" Data="{StaticResource CrossGeometry}" Stretch="Fill" Margin="16"/>
                            </Grid>
                            <Grid x:Name="CheckGroup" RenderTransformOrigin="0.5,0.5" Visibility="Collapsed">
                                <Grid.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform/>
                                        <SkewTransform/>
                                        <RotateTransform/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Grid.RenderTransform>
                                <Ellipse x:Name="CheckNought" Stroke="#00FFFFFF" StrokeThickness="4" Margin="15" noesis:Element.PPAAIn="0"/>
                                <Path x:Name="CheckCross" Fill="#00FFFFFF" Data="{StaticResource CrossGeometry}" Stretch="Fill" Margin="16"/>
                            </Grid>
                        </Grid>
                        <ControlTemplate.Triggers>
                            <DataTrigger Binding="{Binding State}" Value="{x:Static local:State.Player1}">
                                <Setter Property="Visibility" Value="Collapsed" TargetName="OverCross"/>
                            </DataTrigger>
                            <DataTrigger Binding="{Binding State}" Value="{x:Static local:State.Player2}">
                                <Setter Property="Visibility" Value="Collapsed" TargetName="OverNought"/>
                            </DataTrigger>
                            <Trigger Property="IsKeyboardFocused" Value="True">
                                <Trigger.EnterActions>
                                    <BeginStoryboard Storyboard="{StaticResource OverAnimOn}"/>
                                </Trigger.EnterActions>
                                <Trigger.ExitActions>
                                    <BeginStoryboard Storyboard="{StaticResource OverAnimOff}"/>
                                </Trigger.ExitActions>
                            </Trigger>
                            <Trigger Property="Content" Value="{x:Static local:State.Player1}">
                                <Setter Property="Visibility" Value="Collapsed" TargetName="CheckCross"/>
                            </Trigger>
                            <Trigger Property="Content" Value="{x:Static local:State.Player2}">
                                <Setter Property="Visibility" Value="Collapsed" TargetName="CheckNought"/>
                            </Trigger>
                            <Trigger Property="IsChecked" Value="True">
                                <Trigger.EnterActions>
                                    <BeginStoryboard Storyboard="{StaticResource CheckAnimOn}"/>
                                </Trigger.EnterActions>
                                <Trigger.ExitActions>
                                    <BeginStoryboard Storyboard="{StaticResource CheckAnimOff}"/>
                                </Trigger.ExitActions>
                                <Setter Property="Visibility" Value="Collapsed" TargetName="OverGroup"/>
                                <Setter Property="Visibility" Value="Visible" TargetName="CheckGroup"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

